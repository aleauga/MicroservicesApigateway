{
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  },
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/user/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5151
        }
      ],
      "UpstreamPathTemplate": "/user/{everything}",
      "UpstreamHttpMethod": [
        "GET"
      ],
      "SwaggerKey": "user"
    },
    {
      "DownstreamPathTemplate": "/api/products/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5280
        }
      ],
      "UpstreamPathTemplate": "/products/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST"
      ],
      "SwaggerKey": "products"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "user",
      "Config": [
        {
          "Name": "User API",
          "Version": "v1",
          "Url": "http://localhost:5151/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "products",
      "Config": [
        {
          "Name": "Products API",
          "Version": "v1",
          "Url": "http://localhost:5280/swagger/v1/swagger.json"
        }
      ]
    }
  ]

}
